{
  "name": "sweet-bundler",
  "version": "1.0.0",
  "author": {
    "name": "borvelt",
    "email": "borvelt@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "github",
    "url": "https://github.com/borvelt/sweet-bundler.git"
  },
  "description": "https://github.com/borvelt/sweet-bundler.git",
  "main": "dist/index.js",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js . --cache",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "dev": "npm run lint && webpack-dev-server --open --config webpack.config.dev.js",
    "build": "npm run lint && webpack --config webpack.config.prod.js",
    "test": "jest --no-cache"
  },
  "pre-commit": {
    "run": [
      "precommit-msg",
      "lint"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^5.6.0",
    "eslint-plugin-jest": "^21.22.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^21.1.0",
    "pre-commit": "^1.2.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8"
  }
}
