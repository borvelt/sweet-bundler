{
  "name": "es2017-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js . --cache",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "start": "npm run lint && webpack-dev-server --open",
    "build": "webpack",
    "test": "jest"
  },
  "pre-commit": {
    "run": [
      "precommit-msg",
      "lint"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^3.19.0",
    "eslint-plugin-jest": "^21.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.1.0",
    "pre-commit": "^1.2.2",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.8.2"
  }
}
